{
    "boot-source": {
        "kernel_image_path": "./arch-vmlinux.bin",
        "boot_args": "console=ttyS0 noapic reboot=k panic=1 pci=off nomodules 8250.nr_uarts=1 i8042.noaux i8042.nomux i8042.nopnp i8042.dumbkbd root=/dev/vda rw"
    },
    "drives": [
        {
            "drive_id": "rootfs",
            "path_on_host": "./alpine-rootfs",
            "is_root_device": true,
            "is_read_only": false
        }
    ],
    "network-interfaces": [
        {
            "iface_id": "eth0",
            "guest_mac": "AA:FC:00:00:00:01",
            "host_dev_name": "tap0"
        }
    ],
    "machine-config": {
        "vcpu_count": 1,
        "mem_size_mib": 128,
        "ht_enabled": false
    }
}
